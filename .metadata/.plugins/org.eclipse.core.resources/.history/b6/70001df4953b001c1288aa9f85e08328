class Prog 
{
    public static void main(String args[]) 
    {        
         int x = 2;
         int y = 0;
         for ( ; y < 10; ++y) 
         {
        	
             if (y % x == 0) {
            	 System.out.print("Valueof y inter1 fierst block"+y);
            	  continue;  
