class Prog 
{
    public static void main(String args[]) 
    {        
         int x = 2;
         int y = 0;
         for ( ; y < 10; ++y) 
         {
        	 System.out.println(y + "y");
             if (y % x == 0) 
                 continue;  
             else if (y == 8)
                  break;
             else
